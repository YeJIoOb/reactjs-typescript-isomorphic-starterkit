{
	"name": "reactjs-typescript-isomorphic-starterkit",
	"version": "2.0.0",
	"description": "A React.js typescript isomorphic template with Webpack and react-router",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean": "node ./bin/clean.js",
		"postinstall": "npm run deploy",
		"server": "node ./bin/server",
		"build": "npm run clean && node ./bin/bundler/prod.js",
		"deploy": "npm run build && npm run server",
		"watch": "npm run clean && node ./bin/bundler/dev.js && concurrent \"node ./bin/server/hotReload.js\" \"nodemon ./bin/server --watch dist -e js\" --kill-others"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ayoubdev/reactjs-typescript-isomorphic-starterkit.git"
	},
	"bugs": {
		"url": "https://github.com/ayoubdev/reactjs-typescript-isomorphic-starterkit/issues"
	},
	"keywords": [
		"reactjs",
		"webpack",
		"react-router",
		"boilerplate",
		"template",
		"typescript",
		"transpilation",
		"isomorphic",
		"server side rendering",
		"client side rendering",
		"starter",
		"kit",
		"starterkit",
		"starter-kit"
	],
	"author": "Ayoub ADIB <ayoub.development@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"concurrently": "^3.4.0",
		"css-loader": "^0.28.4",
		"del": "^3.0.0",
		"extract-text-webpack-plugin": "^2.1.2",
		"file-loader": "^0.11.2",
		"fs": "0.0.2",
		"node-sass": "^4.5.3",
		"nodemon": "^1.9.2",
		"react-hot-loader": "^1.3.0",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.18.2",
		"ts-loader": "^2.1.0",
		"typescript": "^2.3.4",
		"webpack": "^2.6.1",
		"webpack-dev-server": "^2.4.5"
	},
	"dependencies": {
		"@types/history": "^4.5.2",
		"@types/node": "^7.0.31",
		"@types/react": "^15.0.29",
		"@types/react-dom": "^15.5.0",
		"@types/react-redux": "^4.4.44",
		"@types/react-router": "^4.0.114.0.11",
		"@types/react-router-dom": "^4.0.4",
		"compression": "^1.6.2",
		"css-hot-loader": "^1.1.1",
		"express": "^4.14.0",
		"history": "^4.6.2",
		"react": "^15.1.0",
		"react-dom": "^15.1.0",
		"react-redux": "^5.0.5",
		"react-router": "^4.1.1",
		"react-router-dom": "^4.1.1"
	}
}
